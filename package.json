{
  "name": "back-row",
  "version": "0.0.0",
  "description": "A media server for watching movies and shows in a browser, streamed directly from a server",
  "main": "back-row.js",
  "scripts": {
    "start": "node .",
    "lint": "standard",
    "test": "npm run lint",
    "build": "gulp build",
    "postinstall": "npm run build"
  },
  "browserify": {
    "transform": [
      [
        "stringify",
        [
          ".html"
        ]
      ],
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/blakeembrey/back-row.git"
  },
  "keywords": [
    "movies",
    "shows",
    "torrent",
    "stream",
    "browse"
  ],
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blakeembrey/back-row/issues"
  },
  "homepage": "https://github.com/blakeembrey/back-row",
  "dependencies": {
    "babelify": "^5.0.3",
    "browserify": "^9.0.3",
    "client-oauth2": "^0.2.3",
    "connect-livereload": "^0.5.3",
    "envify": "^3.2.0",
    "es6-promise": "^2.0.1",
    "express": "^4.10.1",
    "extend": "^2.0.0",
    "flux": "^2.0.1",
    "free-style": "^0.1.2",
    "gulp": "^3.8.9",
    "gulp-duration": "0.0.0",
    "gulp-livereload": "^3.8.0",
    "keymirror": "^0.1.0",
    "lru-cache": "^2.5.0",
    "mime": "^1.2.11",
    "minifyify": "^6.2.2",
    "moment": "^2.9.0",
    "normalize.css": "^3.0.2",
    "popsicle": "^0.4.0",
    "popsicle-constants": "^0.1.0",
    "popsicle-prefix": "^0.1.0",
    "popsicle-status": "^0.1.0",
    "pump": "^1.0.0",
    "range-parser": "^1.0.2",
    "react": "^0.12.0",
    "react-list": "^0.2.4",
    "react-router": "^0.12.0",
    "read-torrent": "^1.1.0",
    "request": "^2.47.0",
    "require-dir": "^0.1.0",
    "rimraf": "^2.2.8",
    "standard": "^2.6.2",
    "stringify": "^3.1.0",
    "torrent-stream": "^0.17.0",
    "video.js": "^4.10.2",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.1.1"
  },
  "engines": {
    "node": ">=0.10"
  },
  "standard": {
    "ignore": [
      "build/**/*"
    ]
  }
